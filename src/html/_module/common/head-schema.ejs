<!--構造化データ-->
<script type="application/ld+json">
  {
    "@context": "http://schema.org",
    "@type": "BreadcrumbList",
    "itemListElement": [{
        "@type": "ListItem",
        "position": 1,
        "item": {
          "@id": "<%= commonData.siteDomain %>",
          "name": "<%= commonData.siteName %>"
        }
      },
      {
        "@type": "ListItem",
        "position": 2,
        "item": {
          "@id": "<%= commonData.siteDomain %><%= pageData.path %>",
          "name": "<%= pageData.title %>"
        }
      }
    ]
  }
</script>
