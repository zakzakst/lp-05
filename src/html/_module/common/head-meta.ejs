<!--メタデータ-->
<meta charset="utf-8">
<title><%= pageData.title %>｜<%= commonData.siteName %></title>
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="keywords" content="<%= pageData.keywords %>">
<meta name="description" content="<%= pageData.description %>">
<meta name="viewport" content="width=device-width,initial-scale=1">
<link rel="canonical" href="<%= commonData.siteDomain %><%= pageData.path %>">
<link rel="shortcut icon" href="<%= commonData.shortcutIcon %>" type="image/x-icon">
<link rel="apple-touch-icon" href="<%= commonData.appleIcon %>" type="image/png">

<!--OpenGraph-->
<meta property="og:locale" content="ja_JP">
<meta property="og:title" content="<%= pageData.title %>｜<%= commonData.siteName %>">
<meta property="og:type" content="article">
<meta property="og:url" content="<%= commonData.siteDomain %><%= pageData.path %>">
<meta property="og:site_name" content="<%= commonData.siteName %>">
<meta property="og:image" content="<%= commonData.ogImage %>">
<meta property="og:description" content="<%= pageData.description %>">
